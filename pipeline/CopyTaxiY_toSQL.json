{
	"name": "CopyTaxiY_toSQL",
	"properties": {
		"activities": [
			{
				"name": "Copy data to SQL",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ParquetSource",
						"additionalColumns": [
							{
								"name": "filename",
								"value": "$$FILEPATH"
							}
						],
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"wildcardFolderPath": {
								"value": "@concat('yellow/',pipeline().parameters.path)",
								"type": "Expression"
							},
							"wildcardFileName": "*.parquet",
							"enablePartitionDiscovery": false
						}
					},
					"sink": {
						"type": "SqlServerSink",
						"sqlWriterStoredProcedureName": "[dbo].[CopyToTable]",
						"sqlWriterTableType": "TableType",
						"storedProcedureTableTypeParameterName": "data"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "OpenDatasetNYCTaxiY",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "SqlServerDataset",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"path": {
				"type": "string"
			}
		},
		"annotations": []
	}
}